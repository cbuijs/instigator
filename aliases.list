test3.chrisbuijs.com=4.3.2.1
test4.chrisbuijs.com=NXDOMAIN
test5.chrisbuijs.com=REFUSED
www.test5.chrisbuijs.com=1.2.3.4
company.com=REFUSED
www.company.com=PASSTHRU
test.company.com=www.googleapis.com
.!99999999
